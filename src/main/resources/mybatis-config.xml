<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
							   "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
	<typeAliases>
		<typeAlias alias="auth" type="kr.co.earthnus.user.auth.AuthBean"/>
		<typeAlias alias="member" type="kr.co.earthnus.user.member.MemberBean"/>
		<typeAlias alias="donation" type="kr.co.earthnus.user.donation.DonationBean"/>
		<typeAlias alias="pay" type="kr.co.earthnus.user.pay.PayBean"/>
		<typeAlias alias="goods" type="kr.co.earthnus.user.goods.GoodsBean"/>
		<typeAlias alias="exGoods" type="kr.co.earthnus.user.goods.ExGoodsBean"/>
		<typeAlias alias="camBoard" type="kr.co.earthnus.user.camBoard.camBoardBean"/>
		<typeAlias alias="cheBoard" type="kr.co.earthnus.user.cheBoard.CheBoardBean"/>
		<typeAlias alias="mail" type="kr.co.earthnus.util.MailBean"/>
		<typeAlias alias="adAuth" type="kr.co.earthnus.admin.auth.AdAuthBean"/>
		<typeAlias alias="adMember" type="kr.co.earthnus.admin.member.AdMemberBean"/>
		<typeAlias alias="visit" type="kr.co.earthnus.admin.auth.VisitCountBean"/>
