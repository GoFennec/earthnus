    	<mapper resource="mappings/pay-mapping.xml"/>
    	<mapper resource="mappings/goods-mapping.xml"/>
    	<mapper resource="mappings/adGoods-mapping.xml"/>
    	<mapper resource="mappings/adExGoods-mapping.xml"/>
		<mapper resource="mappings/cheBoard-mapping.xml"/>
		<mapper resource="mappings/adCheBoard-mapping.xml"/>
		<mapper resource="mappings/camBoard-mapping.xml"/>
		<mapper resource="mappings/adCamBoard-mapping.xml"/>
		<mapper resource="mappings/mail-mapping.xml"/>
		<mapper resource="mappings/adMember-mapping.xml"/>
		<mapper resource="mappings/adCheBoard-mapping.xml"/>
	</mappers>

</configuration>